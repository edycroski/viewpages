{
    "name": "delatbabel/viewpages",
    "description": "Support rendering/view of Laravel pages and templates from a database.",
    "keywords": ["laravel", "view", "render", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Del",
            "email": "del@babel.com.au"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "delatbabel/applog": "1.5.*",
        "delatbabel/fluents": "~1.0",
        "delatbabel/site-config": "1.1.*",
        "delatbabel/nestedcategories": "1.1",
        "illuminate/support": "5.1.*",
        "illuminate/view": "5.1.*",
        "rcrowe/twigbridge": "^0.9.2",
        "twig/twig": "1.*",
        "aws/aws-sdk-php": "^3.33",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Delatbabel\\ViewPages\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
